 public void SetTransmission(int t)
 {
 this.t =t;
 }
  
 public void SetColors(String c)
 {
 this.c = c;
 }
 public void SetName(String n)
 {
 this.n= n;
 }
 public String About()
 {
 String nameA  = "Number of doors: " +doors + "Number of cylinder :" +cylinder; 
nameA  = nameA +"Transmission " + transmission;
nameA  = name A +"Color " + color;
 } 
 
 public Automobile(int doors, int cylinders)
 {
   this.d = doors;
   this.c = cylinders;
 }  
public Automobile(int doors,int cylinders,int trans)
